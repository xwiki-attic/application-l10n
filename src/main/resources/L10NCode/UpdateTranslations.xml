  <hidden>false</hidden>
  <content>{{velocity output="false"}}
#set ($showcomments = false)
{{/velocity}}

= Updating Translations

{{velocity}}
#set($trans = $xwiki.parseGroovyFromPage("L10NCode.L10NGroovy"))
$trans.setContext($context, $xwiki)
#set($language = $request.language)
#set($name = $request.name)
#set($app = $request.app)

#if ($request.pages)
  #set($pages = $request.pages.split(","))
  #foreach($page in $pages)
    #set($previoustrans = $request.get("${page}_previoustranslatedtext"))
    #set($newtrans = $request.get("${page}_translatedtext"))
    #set($neworiginaltrans = $request.get("${page}_originaltext"))
    #set($validatewithnochange = "")
    #set($validatewithnochange = $request.get("${page}_validatewithnochange"))
    #if ($previoustrans != $newtrans || $validatewithnochange == '1')
      * Updating $page {{code language="none"}}$newtrans{{/code}}
      #set($transdoc = $xwiki.getDocument($page))
      #set($ok = $transdoc.use("L10NCode.TranslationClass"))
      #if (!$transdoc.getValue("validatedtransatedtext") || $transdoc.getValue("validatedtransatedtext") == "")
        #set($ok = $transdoc.set("validatedtranslatedtext", $transdoc.getValue("translatedtext")))
      #end 
      #set($ok = $transdoc.set("translatedtext", $newtrans))
      #set($ok = $transdoc.set("originaltext", $neworiginaltrans))
      #set($ok = $transdoc.set("status", "needreview"))
      $transdoc.save()
    #end
  #end
#elseif ($request.batchimportcontent)
  #set($properties = $trans.getPropertiesFromString($request.batchimportcontent))
  #foreach ($mapEntry in $properties.entrySet())
    #set($result = $services.query.xwql('from doc.object(L10NCode.TranslationClass) as tr where tr.key = :key and tr.language = :language and tr.name = :name').bindValue('key', $mapEntry.key).bindValue('language', $language).bindValue('name', $name).execute())
    #if ($result.empty)
      * {{error}}Key "$mapEntry.key" does not exists{{/error}}
    #else
      #set($trDocument = $xwiki.getDocument($result.get(0)))
      $trDocument.set('translatedtext', $mapEntry.value)
      $trDocument.save()
      * {{success}}Updated translation [[$mapEntry.key&gt;&gt;$trDocument]] ({{{$mapEntry.value}}}){{/success}}
    #end
  #end
#end

## clear cache
$trans.clearCache($app, $name, $language)

[[Back to empty translations page&gt;&gt;${name}?action=viewempty&amp;language=$language]] - [[Back to view all translations page&gt;&gt;${name}?action=viewall&amp;language=$language]] - [[Back to outdated translations page&gt;&gt;${name}?action=viewoutdated&amp;language=$language]] - [[Back to translation file&gt;&gt;$name]]
{{/velocity}}</content>
